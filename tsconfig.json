{
  "compilerOptions": {
    "baseUrl": ".",
    "inlineSourceMap": true,
    "inlineSources": true,
    "module": "ESNext",
    "target": "ES2020",
    "allowJs": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "isolatedModules": true,
  "strictNullChecks": true,
    "lib": [
      "DOM",
      "ES2020"
    ],
  "jsx": "react-jsx",
  "esModuleInterop": true,
  "strict": true
  },
  "include": [
  "src/**/*.ts",
  "src/**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
